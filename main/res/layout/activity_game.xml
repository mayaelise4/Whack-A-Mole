	app:layout_constraintBottom_toBottomOf = "@+id/pause_view"
	app:layout_constraintEnd_toEndOf = "@+id/pause_view"
	app:layout_constraintHorizontal_bias = "0.504"
	app:layout_constraintStart_toStartOf = "@+id/pause_view"
	app:layout_constraintTop_toTopOf = "parent"
	app:layout_constraintVertical_bias = "0.435"
